{
    "name": "molam-platform-backend",
    "version": "1.0.0",
    "description": "Backend Molam Platform - Module Profil Utilisateur",
    "main": "dist/index.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "migrate": "psql $DATABASE_URL -f sql/migrations/029_user_profile.sql",
        "lint": "eslint src/**/*.ts",
        "clean": "rm -rf dist"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.919.0",
        "@aws-sdk/s3-request-presigner": "^3.919.0",
        "cors": "^2.8.5",
        "dotenv": "^16.6.1",
        "express": "^4.21.2",
        "express-rate-limit": "^7.5.1",
        "helmet": "^7.2.0",
        "jsonwebtoken": "^9.0.2",
        "pg": "^8.16.3",
        "sharp": "^0.32.6",
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "@types/cors": "^2.8.19",
        "@types/express": "^4.17.25",
        "@types/jest": "^29.5.14",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node": "^20.19.24",
        "@types/pg": "^8.15.6",
        "jest": "^29.7.0",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.9.3"
    },
    "keywords": [
        "molam",
        "profile",
        "backend",
        "typescript"
    ],
    "author": "Molam Team",
    "license": "MIT",
    "directories": {
        "test": "tests"
    }
