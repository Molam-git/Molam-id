{
  "version": 2,
  "buildCommand": "cd brique-36-ui-id/web && npm run build",
  "outputDirectory": "brique-36-ui-id/web/dist",
  "devCommand": "cd brique-36-ui-id/web && npm run dev",
  "framework": "vite",
  "installCommand": "cd brique-36-ui-id/web && npm install",
  "env": {
    "VITE_API_URL": "https://molam-id.onrender.com"
  },
  "routes": [
    {
      "src": "/[^.]+",
      "dest": "/index.html",
      "status": 200
    }
  ]
}
