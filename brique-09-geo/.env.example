# Service configuration
NODE_ENV=development
PORT=3009

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/molam_id

# Redis
REDIS_URL=redis://localhost:6379

# JWT (public key for verification)
JWT_PUBLIC_KEY="-----BEGIN PUBLIC KEY-----\nMII...\n-----END PUBLIC KEY-----"
JWT_AUDIENCE=molam-id
JWT_ISSUER=https://id.molam.com

# AWS KMS
AWS_REGION=eu-west-3
KMS_KEY_ID=arn:aws:kms:eu-west-3:ACCOUNT_ID:key/KEY_ID

# MaxMind GeoIP2
MAXMIND_ACCOUNT_ID=123456
MAXMIND_LICENSE_KEY=YOUR_LICENSE_KEY
MAXMIND_DB_PATH=/data/GeoLite2-City.mmdb
MAXMIND_ASN_DB_PATH=/data/GeoLite2-ASN.mmdb

# USSD
USSD_WEBHOOK_SECRET=your_webhook_secret_here

# Audit service
AUDIT_SERVICE_URL=http://localhost:3006
