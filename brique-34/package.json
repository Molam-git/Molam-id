{
    "name": "molam-id-sessions",
    "version": "1.0.0",
    "description": "Molam ID Session Monitoring Module",
    "main": "dist/src/app.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/src/app.js",
        "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint src/**/*.ts tests/**/*.ts",
        "lint:fix": "eslint src/**/*.ts tests/**/*.ts --fix",
        "type-check": "tsc --noEmit"
    },
    "dependencies": {
        "express": "^4.18.0",
        "cors": "^2.8.5",
        "helmet": "^7.0.0",
        "express-rate-limit": "^6.8.1",
        "jsonwebtoken": "^9.0.0",
        "redis": "^4.6.0",
        "pg-promise": "^11.0.0",
        "date-fns": "^2.30.0",
        "pg-monitor": "^1.4.0",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.0",
        "@types/cors": "^2.8.0",
        "@types/helmet": "^4.0.0",
        "@types/jsonwebtoken": "^9.0.0",
        "@types/node": "^18.0.0",
        "@types/pg": "^8.0.0",
        "@types/redis": "^4.0.0",
        "@types/uuid": "^9.0.0",
        "@types/jest": "^29.0.0",
        "@types/supertest": "^2.0.0",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^8.0.0",
        "jest": "^29.0.0",
        "supertest": "^6.0.0",
        "ts-jest": "^29.0.0",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.0.0"
    }
}